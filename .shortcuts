# vim: filetype=sh
alias h="cd ~/ && ls -a" \
	d="cd ~/Documents && ls -a" \
	D="cd ~/Downloads && ls -a" \
	m="cd ~/Music && ls -a" \
	pp="cd ~/Pictures && ls -a" \
	vv="cd ~/Videos && ls -a" \
	cf="cd ~/.config && ls -a" \
	sc="cd ~/.scripts && ls -a" \
	mn="cd /mnt && ls -a" \
	cdz="$EDITOR ~/.zshrc" \
	cfv="$EDITOR ~/.config/nvim/init.vim" \
	cfr="$EDITOR ~/.config/ranger/rc.conf" \
	cfi="$EDITOR ~/.config/i3/config" \
	cfd="$EDITOR ~/.Xdefaults" 
	
